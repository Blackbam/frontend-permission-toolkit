<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>

            <!-- the plugin name [A-Za-z_] -->
            <pluginName>FrontendPermissionToolkit</pluginName>

            <!-- a short description -->
			<pluginDescription><![CDATA[]]></pluginDescription>

            <!-- meta data -->
            <pluginVersion>1.0</pluginVersion>
            <pluginRevision>1</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <!-- put in the URL for the plugin configuration eg. /plugin/FrontendPermissionToolkit/admin/index -->
            <pluginIframeSrc></pluginIframeSrc>

            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>FrontendPermissionToolkit_Plugin</pluginClassName>

            <!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/FrontendPermissionToolkit/lib</path>
			</pluginIncludePaths>

			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>FrontendPermissionToolkit</namespace>
			</pluginNamespaces>

			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths>
                <path>/plugins/FrontendPermissionToolkit/static/js/startup.js</path>

                <path>/plugins/FrontendPermissionToolkit/static/js/datatypes/classes/data/permissionResource.js</path>
                <path>/plugins/FrontendPermissionToolkit/static/js/datatypes/tags/permissionResource.js</path>

                <path>/plugins/FrontendPermissionToolkit/static/js/datatypes/classes/data/permissionObjects.js</path>
                <path>/plugins/FrontendPermissionToolkit/static/js/datatypes/tags/permissionObjects.js</path>

                <path>/plugins/FrontendPermissionToolkit/static/js/datatypes/classes/data/permissionHref.js</path>
                <path>/plugins/FrontendPermissionToolkit/static/js/datatypes/tags/permissionHref.js</path>

			</pluginJsPaths>

            <pluginJsPaths-extjs6>
                <path>/plugins/FrontendPermissionToolkit/static6/js/startup.js</path>

                <path>/plugins/FrontendPermissionToolkit/static6/js/datatypes/classes/data/permissionResource.js</path>
                <path>/plugins/FrontendPermissionToolkit/static6/js/datatypes/tags/permissionResource.js</path>

                <path>/plugins/FrontendPermissionToolkit/static6/js/datatypes/classes/data/permissionObjects.js</path>
                <path>/plugins/FrontendPermissionToolkit/static6/js/datatypes/tags/permissionObjects.js</path>

                <path>/plugins/FrontendPermissionToolkit/static6/js/datatypes/classes/data/permissionHref.js</path>
                <path>/plugins/FrontendPermissionToolkit/static6/js/datatypes/tags/permissionHref.js</path>

            </pluginJsPaths-extjs6>

			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths>
                <path>/plugins/FrontendPermissionToolkit/static/css/backend.css</path>
			</pluginCssPaths>

            <pluginCssPaths-extjs6>
                <path>/plugins/FrontendPermissionToolkit/static6/css/backend.css</path>
            </pluginCssPaths-extjs6>
		</plugin>
</zend-config>
